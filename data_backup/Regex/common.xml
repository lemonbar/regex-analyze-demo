    (?:19[5-9]|20[01])\d(?:\s*年)?(?:[-—年./\s]+(?:1[0-2]|0?[1-9])(?:\s*月)?(?:[-—月./\s]+(?:[12]\d|3[01]|0?[1-9])(?:(?:(?<=1)st|(?<=2)nd|(?<=3)rd|(?<![123])th)\b|\s*[日号])?)?)?
        |
    (?:[89]|[01])\d[-—年./\s]+(?:1[0-2]|0?[1-9])(?:\s*月)?(?:[-—月./\s]+(?:[12]\d|3[01]|0?[1-9])(?:(?:(?<=1)st|(?<=2)nd|(?<=3)rd|(?<![123])th)\b|\s*[日号])?)?
        |
    (?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|June?|July?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)
        \.?(?:\s+(?:[12]\d|3[01]|0?[1-9])(?:(?:(?<=1)st|(?<=2)nd|(?<=3)rd|(?<![123])th)\b|\s*[日号])?)?[\s,]+(?:(?:19[5-9]|20[01])\d|(?:[89]|[01])\d)
