(?<!(?:server|windows|delphi)[ ]*|/)
(?<=\b|[至到])
(?:
    (?:1[0-2]|0?[1-9])(?:[/.](?:[12]\d|3[01]|0?[1-9]))?[/.](?:(?:19[5-9]|20[01])\d|(?:[89]|[01])\d)
