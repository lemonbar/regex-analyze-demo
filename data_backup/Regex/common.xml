    (?:[12]\d|3[01]|0?[1-9])(?:(?:(?<=1)st|(?<=2)nd|(?<=3)rd|(?<![123])th)\b|\s*[日号])?[\s-]+
        (?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|June?|July?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)
        [\s,.-]+(?:(?:19[5-9]|20[01])\d|(?:[89]|[01])\d)
)
(?=\b|[至到在])(?![ ]*年)(?!-[3-9])
  |
(?:现在|当前|目前|至今|今|\b(?:now|present))\b
)
